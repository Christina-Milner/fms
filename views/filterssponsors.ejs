<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Fen Model Show</title>
    <link rel="stylesheet" href="css/style.css">
    <script src="js/main.js" defer></script>
</head>

<body>    
    <% if (isAuthenticated) { %>
        <nav class="pageNav">
            <%- include('partials/navAuth.ejs') %>
        </nav>

        <h1>Miniatures Show Results</h1>
            <%- include('partials/filterbuttons.ejs') %>
            <div class="entries">
                <%  for (let sponsor in info) {%>
                    <h2><%= sponsor %></h2>
                    <% for (let winner of info[sponsor]) {%>
                        <div class="entry" id=<%= winner.id %>>
                            <div>
                                <p><%= winner.junior ? "Junior" : "Adult" %></p>
                            </div>
                            <div>
                                <p>Number:</p>
                                <p>Name:</p>
                            </div>
                            <div>
                                <p><%= winner.id %></p>
                                <p><%= winner.fullName %></p>
                            </div>
                        </div>
                    <% } %>                        
                <% } %>
    <% } else { %>
        <nav class="pageNav">
            <%- include('partials/navNoAuth.ejs') %>
        </nav>
        <%- include('partials/error.ejs') %>
    <% } %>
</body>